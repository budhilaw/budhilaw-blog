{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "budhilaw-blocks/header",
  "version": "1.0.0",
  "title": "Budhilaw Custom Header Page",
  "description": "Add a section for header page",
  "supports": {
    "align": [
      "full"
    ]
  },
  "attributes": {
    "title": {
      "type": "string",
      "default": "Header"
    },
    "subtitle": {
      "type": "string",
      "default": "Subtitle"
    },
    "align": {
      "type": "string",
      "default": "full"
    }
  },
  "textdomain": "budhilaw",
  "editorScript": "file:./index.js",
  "render": "file:./render.php"
}
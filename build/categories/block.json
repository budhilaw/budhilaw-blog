{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "budhilaw-blocks/categories",
  "version": "1.0.0",
  "title": "Budhilaw Categories",
  "description": "Add a section with a title and a list of categories",
  "supports": {
    "align": [
      "full"
    ]
  },
  "attributes": {
    "title": {
      "type": "string",
      "default": "Categories"
    },
    "align": {
      "type": "string",
      "default": "full"
    }
  },
  "textdomain": "budhilaw",
  "editorScript": "file:./index.js",
  "render": "file:./render.php"
}